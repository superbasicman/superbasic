{
  "version": 2,
  "buildCommand": "cd ../.. && turbo run build --filter=@repo/web",
  "outputDirectory": "dist",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "ignoreCommand": "npx turbo-ignore",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
